{
  "name": "promise-queue",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon --exec \"ts-node --esm --project tsconfig.util.json src/index.ts\"",
    "dev-silent": "nodemon --exec \"ts-node --esm --logError --project tsconfig.util.json src/index.ts\"",
    "dev-inspect": "cross-env NODE_OPTIONS=\"--inspect-brk\" ts-node --esm --project tsconfig.util.json src/index.ts",
    "dev-showConfig": "ts-node --esm --project tsconfig.util.json --showConfig src/index.ts",
    "test": "cross-env TS_NODE_PROJECT=\"./tsconfig.tests.json\" nodemon --exec mocha",
    "test-once": "cross-env TS_NODE_PROJECT=\"./tsconfig.tests.json\" mocha",
    "lint": "nodemon --exec \"yarn run lint-once\"",
    "lint-once": "eslint . --ext .ts,.tsx,.cts,.mts,.js,.jsx,.cjs,.mjs",
    "clean": "rimraf dist",
    "build": "tsc -b",
    "build-dry": "tsc -b --dry",
    "build-verbose": "tsc -b --verbose",
    "build-listFilesOnly": "tsc -b --listFilesOnly",
    "build-and-start": "yarn run build && yarn run start",
    "build-util": "tsc -b tsconfig.util.json",
    "build-tests": "tsc -b tsconfig.tests.json",
    "clean-util": "tsc -b tsconfig.util.json --clean",
    "clean-tests": "tsc -b tsconfig.tests.json --clean",
    "tsc-p-util": "tsc -p tsconfig.util.json",
    "tsc-p-tests": "tsc -p tsconfig.tests.json",
    "util-showConfig:tsc": "tsc -p tsconfig.util.json --showConfig",
    "tests-showConfig:tsc": "tsc -p tsconfig.tests.json --showConfig",
    "search": "wrap () { grep -r --exclude-dir=\"node_modules\" --exclude-dir=\"dist\" --exclude-dir=\".ts-node\" --exclude=\"yarn.lock\" --exclude=\".git\" -- \"$*\" .; }; wrap",
    "search-i": "wrap () { grep -r -i --exclude-dir=\"node_modules\" --exclude-dir=\"dist\" --exclude-dir=\".ts-node\" --exclude=\"yarn.lock\" --exclude=\".git\" -- \"$*\" .; }; wrap",
    "coverage": "cross-env TS_NODE_PROJECT=\"./tsconfig.tests.json\" c8 mocha"
  },
  "devDependencies": {
    "@tsconfig/node-lts-strictest-esm": "^18.12.1",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/deep-equal-in-any-order": "^1.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.13.0",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.9",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "c8": "^7.13.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.3",
    "deep-equal-in-any-order": "^2.0.3",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-define-config": "^1.15.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "sinon": "^15.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {}
}
